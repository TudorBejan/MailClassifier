YUI.add("tictac-att-progressbar",function(a){function b(){}b.prototype={render:function(b){var c=this;c.bar=a.Node.create('<span><span class="tt-progressbar-bar"></span></span>'),c.rail=a.Node.create('<div class="tt-progressbar-rail"></div>'),c.rail.append(c.bar),a.one(b).append(c.rail),c.width=c.rail.get("width"),c.set(0)},set:function(b){b=parseFloat(b),a.Lang.isNumber(b)&&this.bar.setStyle("width",Math.max(0,Math.min(100,b))+"%")},cleanup:function(){var a=this;a.bar.remove(!0),a.rail.remove(!0),a.bar=null,a.rail=null}},a.namespace("Tictac.widget").Progressbar=b},"1.0.0",{requires:["node"]});